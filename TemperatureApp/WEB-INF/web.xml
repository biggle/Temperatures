<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<display-name>Temperature Demo</display-name>
	<description>Servlet Demos.</description>

	<!-- Define servlets that are included in the example application -->

	<servlet>
		<servlet-name>ReserveServlet</servlet-name>
		<servlet-class>servlet.ReserveServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ReserveServlet</servlet-name>
		<url-pattern>/reserveEvent</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>SearchServlet</servlet-name>
		<servlet-class>servlet.SearchServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SearchServlet</servlet-name>
		<url-pattern>/searchEvents</url-pattern>
	</servlet-mapping>

</web-app>